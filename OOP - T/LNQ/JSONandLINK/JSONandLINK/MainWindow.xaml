<Window x:Class="JSONandLINK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSONandLINK"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Margin="10">
        <GroupBox Header="Choose File"></GroupBox>
        <StackPanel>
            <Button Name="btnChooseFile" Click="BtnChooseFile_OnClick">Choose File</Button>
            <Label Name="lblFile">File: ...</Label>
            
            </StackPanel>
        <GroupBox Header="Text To Find">
            <TextBox Name="tbxTextToFind" IsEnabled="False"  TextChanged="TbxTextToFind_OnTextChanged" ></TextBox>
            
            
        </GroupBox>
        <GroupBox Header="Tree Info" Height="192" Margin="0,0,0.4,0">
            
            <ListBox Name="lbxTreeInfo" Margin="0,0,-0.2,12.6">...</ListBox>        
            
        </GroupBox>
    </StackPanel>
</Window>
